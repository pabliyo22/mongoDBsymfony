                    <tr>
                        <th>{{'idEGM' | trans}}</th>
                        <th>{{'nroPres'|trans }}</th>
                        <th>{{'sequence' | trans}}</th>
                        <th>{{'denomination' | trans}}</th>  
                        <th>{{'win' | trans}}</th>
                        <th>{{'DateSecStar' | trans}}</th>

                        <th>{{'jj' | trans}}</th>
                        <th>{{'coinIn' | trans}}</th>
                        <th>{{'coinOut' | trans}}</th>
                        <th>{{'jackpot' |trans}}</th>                
                        <th>{{'DateSecStar'}} |trans</th>

                        <th>{{'jj' |trans }}</th>
                        <th>{{'coinIn' |trans}}</th>
                        <th>{{'coinOut' |trans}}</th>
                        <th>{{'jackpot' |trans }}</th>                
                    </tr> 
                </thead>
                <tbody id="contentTableUser">
                    {% for ship in list %}  

                        <tr>
                            <td> 
                                {{ ship.egm   }} 
                            </td>
                            <td>
                                {{ ship.nrosec  }} 
                            </td>
                            <td>
                                {{ ship.nropres  }} 
                            </td>
                            <td> 
                                {{ ship.denomegm }} 
                            </td>
                            <td> 
                                {{ ship.win   }}
                            </td>                      

                            <td >
                                {{ship.FirstCounter[0].dateSeqStart|date('d/m/Y H:i:s')  }}
                            </td>

                            <td >
                                {{ship.FirstCounter[0].jj}}

                            </td>
                            <td >
                                {{ship.FirstCounter[0].ci}}

                            </td>
                            <td >
                                {{ship.FirstCounter[0].co}}

                            </td>
                            <td >
                                {{ship.FirstCounter[0].jp}}
                            </td>       
                            <td >
                                {{ship.LastCounter[0].dateSeqEnd|date('d/m/Y  H:i:s')  }}
                            </td>
                            <td >
                                {{ship.LastCounter[0].jj}}

                            </td>
                            <td >
                                {{ship.LastCounter[0].ci}}

                            </td>
                            <td >
                                {{ship.LastCounter[0].co}}
                            </td>
                            <td >
                                {{ship.LastCounter[0].jp}}
                            </td>

